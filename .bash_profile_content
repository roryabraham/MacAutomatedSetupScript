# Get some syntax highlighing in for commands
export CLICOLOR=1
export LSCOLORS=GxFxCxDxBxegedabagaced

# Setting Sat's color schemes to bash terminal
if [[ $- == *i* ]]; then
	export PS1="\[\033[38;5;39m\]\u\[$(tput sgr0)\]\[\033[38;5;15m\]@\[$(tput sgr0)\]\[\033[38;5;136m\]\h\[$(tput sgr0)\]\[\033[38;5;15m\] \[$(tput sgr0)\]\[\033[38;5;2m\]\W\[$(tput sgr0)\]\[\033[38;5;15m\]>\[$(tput sgr0)\] "
fi